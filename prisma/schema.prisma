generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model couponInfo {
  id                 Int       @id @default(autoincrement())
  dateCreated        DateTime? @db.Timestamp(6)
  uniqueCode         String?   @db.VarChar(21)
  linkUniqueCouponCc String?   @db.VarChar(55)
  verifiedFlag       Int?      @default(0) @db.SmallInt
  verifiedOn         DateTime? @db.Timestamp(6)
  del                Int?      @default(0) @db.SmallInt
  location           String?   @db.VarChar(255)
}

model userInfo {
  id          Int       @id @default(autoincrement())
  dateCreated DateTime? @default(now()) @db.Timestamp(6)
  name        String    @db.VarChar(55)
  email       String    @unique @db.VarChar(150)
  userName    String    @db.VarChar(155)
  password    String    @db.VarChar(155)
  mobile      String?   @db.VarChar(15)
  lastLogin   DateTime? @db.Timestamp(6)
  status      Int?      @default(1) @db.SmallInt
  del         Int?      @default(0) @db.SmallInt
}

model dynamicLinkInfo {
  id         Int    @id @default(autoincrement())
  couponLink String @db.VarChar(150)
  uploadLink String @db.VarChar(150)
}
